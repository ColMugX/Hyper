@import './dialog.styl'
@import './syuanpi.styl'

.confirm
  @extends .dialog
  .confirm-label
    display block
    text-align center
    font-size 12px
    padding 4px 0 8px
    background-image linear-gradient(180deg, #ccc, #ccc 50%, transparent 50%)
    background-size 100% 1px
    background-repeat no-repeat
    background-position bottom left
    background-origin border-box
wrapper('confirm')
